class Point {
      private float x;
      private float y;
      
      public Point(float x, float y) {
        this.x = x;
        this.y = y;
      }
      
      public Point() {
        this(0, 0);
      }
      
      public float getX() {
        return x;
      }
      
      public float getY() {
        return y;
      }
    }
